  "scripts": {
    "start": "react-scripts start",
    "build": "BUILD_PATH='../api/build' react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "doc": "jsdoc -c jsdoc.json",
    "api": "jsdoc-to-readme --src src/index.js",
    "toc": "markdown-toc -i README.md  --maxdepth 3",
    "readme": "npm run api && npm run toc",
    "jsdoc": "jsdoc -a all -c jsdoc.json -r -R README.md index.js -d docs/jsdoc && jsdoc2md --files index.js > docs/API.md"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "docdash": "^2.0.2",
    "jsdoc": "^4.0.3",
    "jsdoc-to-markdown": "^8.0.1",
    "jsdoc-to-readme": "^2.0.2",
    "markdown-toc": "^1.2.0"
  }
}
